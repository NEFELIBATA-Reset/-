class Solution {
    public int removeElement(int[] nums, int val) {
        int j=0;//定义一个元素从0开始遍历
        for(int i=0;i<nums.length;i++){//遍历数组
            if(nums[i]!=val){//如果数组元素不等于给定元素
                nums[j]=nums[i];//就将元素赋给新的下标元素
                j++;//让j++
            }
        }

    
        return j;
    } 
}
